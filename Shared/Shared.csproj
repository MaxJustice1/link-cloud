<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>LantanaGroup.Link.Shared</RootNamespace>
    <Configurations>Debug;Release;Debug IIS</Configurations>
  </PropertyGroup>

	<PropertyGroup>
		<Authors>Lantana Consulting Group</Authors>
		<Company>Lantana Consulting Group</Company>
		<Description>Shared library for Lantana Link</Description>
		<Product>NHSN Link Shared Library</Product>
		<Version>1.1.1</Version>
		<PackageId>com.lantanagroup.link.Shared</PackageId>
	</PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AutoMapper" Version="13.0.1" />
    <PackageReference Include="Confluent.Kafka" Version="2.3.0" />
    <PackageReference Include="Confluent.Kafka.Extensions.Diagnostics" Version="0.4.0" />
    <PackageReference Include="Google.Protobuf" Version="3.25.3" />
    <PackageReference Include="Hl7.Fhir.R4" Version="5.6.1" />
    <PackageReference Include="Microsoft.Extensions.Options" Version="8.0.2" />
    <PackageReference Include="MongoDB.Driver" Version="2.24.0" />
    <PackageReference Include="StackExchange.Redis" Version="2.7.20" />
    <PackageReference Include="System.Text.Encodings.Web" Version="8.0.0" />
  </ItemGroup>

	<ItemGroup>
		<FrameworkReference Include="Microsoft.AspNetCore.App" />
	</ItemGroup>

</Project>
